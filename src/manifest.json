{
   
    "name":"ShortCutEverything",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_title": "Bear",
      "default_popup": "popup.html"
    },

    "permissions": ["tabs", "storage"]

    
  }